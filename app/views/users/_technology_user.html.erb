<a class="tech-panel">
  <% if progress.technology.icon.attached? %>
  <%= image_tag progress.technology.icon, class: 'tech-icon' %>
  <% else %>
  <%= image_tag 'no-photo.jpg', class: 'tech-icon' %>
  <% end %>
    <div class="tech-info">
    <h2 class="tech-name"> <%= progress.technology.title %> </h2>
    <p class="tech-description"> <%= progress.technology.discription %> </p>
    <p class="tech-description"> <%= progress.progress %> </p>
  </div>
</a>
