<a class="tech-panel" href="<%= technology_path(progress.technology) %>">
    <div class="tech-progress" style="color: rgb( <%= 255 - progress.progress*255/100 %> , <%= progress.progress*255/100 %> , 0)">
      <%= progress.progress %>%
    </div>
    <div class="tech-info">
    <h2 class="tech-name"> <%= progress.technology.title %> </h2>
    <p class="tech-description"> <%= progress.technology.discription %> </p>
  </div>
</a>
