<div class="notification">
  <h1>Are you sure that you want to start?</h1>
  <%= link_to 'start test',
              test_start_path(@test.technology, @test), method: :post %>
  <%= link_to 'back to all tests',
              technology_path(@test.technology) %>
</div>
<% if current_user.admin? %>
<div class="tests-container">
  <div class="test-form">
    <h1>edit Test:</h1>
    <%= render 'form' %>
  </div>
  <%= react_component('test/QuestionList', {technology_id: @test.technology.id, test: @test}) %>
</div>
<% end %>
